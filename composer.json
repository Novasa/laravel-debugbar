{
    "name": "novasa/laravel-debugbar",
    "description": "PHP Debugbar integration for Laravel.",
    "keywords": ["laravel", "debugbar", "profiler", "debug", "webprofiler"],
    "license": "MIT",
	"repositories": [
        {
            "type": "git",
            "url": "https://github.com/novasa/php-debugbar.git"
        }
    ],

    "authors": [
        {
            "name": "Barry vd. Heuvel",
            "email": "barryvdh@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "laravel/framework": "~4.0",
        "symfony/finder": "~2.3",
        "novasa/debugbar": "1.9.*@dev"
    },
    "autoload": {
        "psr-0": {
            "Barryvdh\\Debugbar": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.6-dev"
        }
    }
}
